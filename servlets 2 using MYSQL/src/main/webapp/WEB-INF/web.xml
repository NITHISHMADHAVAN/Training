<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<listener>
		<listener-class>listener.Loggers</listener-class>
	</listener>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<display-name>login</display-name>
		<description></description>
		<servlet-class>services.login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>example</servlet-name>
		<display-name>example</display-name>
		<description></description>
		<servlet-class>services.example</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Update</servlet-name>
		<display-name>Update</display-name>
		<description></description>
		<servlet-class>services.Update</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Delete_Session</servlet-name>
		<display-name>Delete_Session</display-name>
		<description></description>
		<servlet-class>services.Delete_Session</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>example</servlet-name>
		<url-pattern>/example</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Update</servlet-name>
		<url-pattern>/Update</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Delete_Session</servlet-name>
		<url-pattern>/Delete_Session</url-pattern>
	</servlet-mapping>
	
</web-app>
